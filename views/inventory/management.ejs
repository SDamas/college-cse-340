<section id="management-container">
  <% if (title) { %>
    <h1><%= title %></h1>
  <% } else {
    res.redirect('/')
  } %>

  <%- messages() %>

  <% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>

  <div id="management-actions">
    <a href="/inv/add-classification">Add classification</a>
    <a href="/inv/add-vehicle">Add inventory</a>
  </div>
</section>